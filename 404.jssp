<?
	var querystring = require('querystring');

	POST['filename'] = querystring.unescape(POST['filename']);
	var filename = POST['filename'];
	if('index.jssp'==filename) filename=POST['filename']='index';

	if(filename.indexOf('.')>=0) include('download.jssp');
	else
	{
		if(GET['p']=='edit') include('edit.jssp');
		else include('show.jssp');
	}